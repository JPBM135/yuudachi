{
	"name": "yuudachi",
	"version": "3.0.0",
	"description": "",
	"scripts": {
		"build:clean": "del-cli dist",
		"build:check": "tsc --noEmit",
		"build:esm": "swc ./src --out-dir ./dist",
		"build:copy-locales": "cpy locales/ dist/",
		"build": "yarn workspace @yuudachi/framework run build && yarn build:clean && yarn build:check && yarn build:esm && yarn build:copy-locales",
		"lint": "prettier --check . && cross-env TIMING=1 eslint src --ext ts --format=pretty",
		"format": "prettier --write . && cross-env TIMING=1 eslint src --ext ts --fix --format=pretty",
		"fmt": "yarn format",
		"start": "env-cmd node --enable-source-maps dist/index.js",
		"start:dev": "npm run build && npm run dev",
		"dev": "env-cmd node --enable-source-maps dist/index.js",
		"deploy:commands": "env-cmd node --enable-source-maps dist/deploy.js",
		"migrate": "env-cmd ley"
	},
	"type": "module",
	"directories": {
		"lib": "src"
	},
	"keywords": [],
	"author": "iCrawl <icrawltogo@gmail.com>",
	"license": "AGPL-3.0",
	"private": true,
	"dependencies": {
		"@aero/sanitizer": "^1.2.0",
		"@discordjs/rest": "^1.6.0",
		"@naval-base/ms": "^3.1.0",
		"@skyra/i18next-backend": "^2.0.1",
		"@swc/helpers": "^0.4.14",
		"@yuudachi/framework": "workspace:^",
		"bufferutil": "^4.0.7",
		"bullmq": "^3.10.1",
		"common-tags": "^1.8.2",
		"dayjs": "^1.11.7",
		"diff": "^5.1.0",
		"discord-api-types": "^0.37.36",
		"discord.js": "^14.8.0",
		"env-cmd": "^10.1.0",
		"fastest-levenshtein": "^1.0.16",
		"i18next": "^22.4.12",
		"ioredis": "^5.3.1",
		"kleur": "^4.1.5",
		"nanoid": "^4.0.1",
		"postgres": "^3.3.4",
		"prom-client": "^14.2.0",
		"re2": "^1.18.0",
		"readdirp": "^3.6.0",
		"redis": "^4.6.5",
		"reflect-metadata": "^0.1.13",
		"tsyringe": "^4.7.0",
		"undici": "^5.21.0",
		"utf-8-validate": "^6.0.3",
		"ws": "^8.13.0"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.62",
		"@swc/core": "^1.3.41",
		"@types/aero__sanitizer": "^1.2.0",
		"@types/common-tags": "^1.8.1",
		"@types/diff": "^5.0.2",
		"@types/node": "^18.15.5",
		"@types/ws": "^8.5.4",
		"cpy-cli": "^4.2.0",
		"cross-env": "^7.0.3",
		"del-cli": "^5.0.0",
		"eslint": "^8.36.0",
		"eslint-config-neon": "^0.1.41",
		"eslint-formatter-pretty": "^5.0.0",
		"ley": "^0.8.1",
		"prettier": "^2.8.5",
		"type-fest": "^3.6.1",
		"typescript": "^5.0.2"
	},
	"engines": {
		"node": ">=16.0.0"
	}
}
